{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "remix build",
    "dev": "concurrently npm:dev:remix npm:dev:tsc",
    "dev:remix": "remix dev",
    "dev:tsc": "tsc-watch --noClear",
    "dev:prettier": "prettier --end-of-line lf --write \"app/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "dev:prettier-comment": "Not including dev:prettier in dev, as you should be doing that in your IDE",
    "check:type": "tsc -b",
    "check:prettier": "prettier --end-of-line lf \"app/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "pako": "^2.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@types/pako": "^1.0.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "prettier": "^2.5.1",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
